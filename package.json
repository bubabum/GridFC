{
	"name": "gridfc",
	"version": "1.0.0",
	"description": "application",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"build": "electron-builder"
	},
	"build": {
		"productName": "GridFC",
		"appId": "GridFC",
		"mac": {
			"category": "your.app.category.type"
		},
		"win": {
			"target": "nsis",
			"icon": "./img/logo.png"
		},
		"directories": {
			"output": "release-installer",
			"app": ""
		},
		"nsis": {
			"allowElevation": true,
			"createDesktopShortcut": true,
			"createStartMenuShortcut": true,
			"runAfterFinish": true,
			"deleteAppDataOnUninstall": true,
			"oneClick": false,
			"perMachine": true
		},
		"publish": [
			{
				"provider": "generic",
				"url": ""
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bubabum/GridFC.git"
	},
	"author": "Mykola Kyslyi",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/bubabum/GridFC/issues"
	},
	"homepage": "https://github.com/bubabum/GridFC#readme",
	"dependencies": {
		"pdfkit": "^0.13.0"
	},
	"devDependencies": {
		"electron": "^18.0.3",
		"electron-builder": "^23.0.3"
	}
}